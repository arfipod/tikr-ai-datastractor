{
  "manifest_version": 3,
  "name": "TIKR → AI Data Extractor",
  "version": "1.1.0",
  "description": "Extract all financial data from TIKR Terminal as plain text for AI analysis.",
  "permissions": ["activeTab", "scripting", "clipboardWrite", "storage", "sidePanel", "tabs"],
  "host_permissions": ["https://app.tikr.com/*"],
  "action": { "default_title": "TIKR → AI" },
  "side_panel": { "default_path": "popup.html" },
  "background": { "service_worker": "background.js" }
}